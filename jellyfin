networks:
  traefik_default:
    external: true

version: "3.5"
services:
  jellyfin:
    image: jellyfin/jellyfin
    container_name: jellyfin
    user: 0:0
    ports:
      - "8096:8096"
    networks:
      - traefik_default
    volumes:
      - /root/docker/jellyfin/config:/config
      - /root/docker/jellyfin/cache:/cache
      - /root/docker/jellyfin/media/media
    restart: "unless-stopped"
    labels:
      - "traefik.enable=true"
      - "traefik.http.routers.jellyfin.rule=Host(`file.mrbuttlicker.xyz`)"
      - "traefik.http.services.jellyfin-websecure.loadbalancer.server.port=8096"
      - "traefik.http.routers.jellyfin.tls=true"


